                'thread_type': $target.eq(0).parents('.interview').data('threadType'),
            });
        }).on("click", '.j_btn_smile', function () {
            var $target = $(this);
            $.stats.track(locate, "直播贴-表情面板按钮", 'frs', 'click', {
                'obj_task': 'common_click',
                'page_type': 'a006',
                'obj_isad': 1,
                'obj_floor': 0,
                'obj_id': 17019292,
                'obj_locate': 13,
                'thread_type': $target.eq(0).parents('.interview').data('threadType'),
            });
        }).on("click", "#listSubmitBtn", function () {
            var $target = $(this);
            $.stats.track(locate, "直播贴-发表按钮", 'frs', 'click', {
                'obj_task': 'common_click',
                'page_type': 'a006',
                'obj_isad': 1,
                'obj_floor': 0,
                'obj_id': 17019292,
                'obj_locate': 14,
                'thread_type': $target.eq(0).parents('.interview').data('threadType'),
            });
        }).on("click", "#j_ed_container", function () {
            var $target = $(this);
            $.stats.track(locate, "直播贴-评论框", 'frs', 'click', {
                'obj_task': 'common_click',
                'page_type': 'a006',
                'obj_isad': 1,
                'obj_floor': 0,
                'obj_id': 17019292,
                'obj_locate': 8,
                'thread_type': $target.eq(0).parents('.interview').data('threadType'),
            });
        })
        $("#slideBtn").bind("click", function(){
            if($(this).hasClass('down')){
                $editor.slideUp();
                $post.slideUp();
                $talk.slideUp();
                $(this).removeClass('down');
                interview.stop();
            }else{
                var $target = $(this);
                $.stats.track(locate, "直播贴-展开弹幕", 'frs', 'click', {
                    'obj_task': 'common_click',
                    'page_type': 'a006',
                    'obj_isad': 1,
                    'obj_floor': 0,
                    'obj_id': 17019292,
                    'obj_locate': 15,
                    'thread_type': $target.eq(0).parents('.interview').data('threadType'),
                });
                if(!$post.hasClass('onlyPost')){
                    $talk.slideDown();
                }
                $post.slideDown();
                $editor.slideDown();
                $(this).addClass('down');
                interview.start();
            }
        });
        $('body').on("click", '.j_emotion', function () {
            var $target = $(this);
            $.stats.track(locate, "直播贴-表情面板选择表情", 'frs', 'click', {
                'obj_task': 'common_click',
                'page_type': 'a006',
                'obj_isad': 1,
                'obj_floor': 0,
                'obj_id': 17019292,
                'obj_locate': 13,
                'thread_type': $target.eq(0).parents('.interview').data('threadType'),
            });
        });
        if(!atInfo)return;
        if(atInfo.isAladdin){
            if(atInfo.uname=='aladdiniframe') {
                interview.scroll2top( $("#threadListGroupCnt") );
            }else{
                if (Boolean("")) {
                    interview.$editorHolder.trigger("click.focus");
                    interview.scroll2editor(215);
                }else{
                    interview.scroll2editor( 200 );
                    setTimeout(function(){
                        interview.shakeEditorBorder();
                    }, 400);
                }
                if(atInfo.uname != null){
                    if(atInfo.pid != null){
                        setTimeout(function(){
                            interview.editor.trigger('atSomeOne', atInfo);
                        }, 200);
                    }
                }
            }
        }
    });
});</script><script>Bigpipe.end();</script></body></html><!--18685667620204043530072217-->
